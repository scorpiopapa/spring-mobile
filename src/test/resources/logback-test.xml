<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %-5level %logger{80} - %msg %n</pattern>
		</encoder>
	</appender>

	<logger name="com.joinway.jiayd" level="debug" />

	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>